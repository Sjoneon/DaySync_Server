# DaySync API 환경설정 파일
# 해당 파일을 .env로 복사하여 사용
# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=1234
DB_NAME=daysync_db

# API 서버 설정
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=True

# 로깅 설정
LOG_LEVEL=INFO
LOG_FILE=daysync_api.log

# CORS 설정 (개발환경)
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,*

# JWT 설정 (향후 사용)
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 외부 API 키 (향후 사용)
NAVER_MAP_CLIENT_ID=your-naver-map-client-id
NAVER_MAP_CLIENT_SECRET=your-naver-map-client-secret
OPENWEATHER_API_KEY=your-openweather-api-key
BUS_API_KEY=your-bus-api-key


